      <Feedback> Recherche de vos tâches</Feedback>
      <VoiceCommandService Target="VoiceCommandBackgroundService"/>
    </Command>-->

        <PhraseList Label="folder">
            <Item>work</Item>
        </PhraseList>
        <PhraseList Label="view">
            <Item>today</Item>
        </PhraseList>
        <PhraseList Label="smartview">
            <Item>tag</Item>
        </PhraseList>
        <PhraseList Label="context">
            <Item>phone</Item>
        </PhraseList>
        <PhraseList Label="tag">
            <Item>tag</Item>
        </PhraseList>

        <PhraseTopic Label="speech" Scenario="Natural Language"></PhraseTopic>

    </CommandSet>
